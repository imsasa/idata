{
  "name"           : "ivalid",
  "version"        : "1.3.0",
  "type"           : "module",
  "description"    : "watch object",
  "main"           : "./src/index.js",
  "scripts"        : {
    "clean" : "rimraf dist",
    "gendoc": "jsdoc -c jsdoc.conf.json",
    "test"  : "cross-env NODE_ENV=test c8 -o ./dist/tests mocha ./tests/*.js",
    "mocha" : "cross-env NODE_ENV=test mocha ./tests/model.js",
    "build" : "webpack"
  },
  "nyc"            : {
    "require"   : [
      "@babel/register"
    ],
    "reporter"  : [
      "lcov",
      "text"
    ],
    "sourceMap" : false,
    "instrument": false
  },
  "repository"     : {
    "type": "git",
    "url" : "git+https://github.com/imsasa/ivalid.git"
  },
  "author"         : "",
  "license"        : "ISC",
  "bugs"           : {
    "url": "https://github.com/imsasa/ivalid/issues"
  },
  "homepage"       : "https://github.com/imsasa/ivalid#readme",
  "devDependencies": {
    "@babel/cli"           : "^7.16.0",
    "@babel/core"          : "^7.16.5",
    "@babel/preset-env"    : "^7.16.5",
    "@babel/register"      : "^7.16.5",
    "babel-plugin-istanbul": "^6.1.1",
    "babel-register"       : "^6.26.0",
    "chai"                 : "^4.3.4",
    "cross-env"            : "^7.0.3",
    "jsdoc"                : "^3.6.3",
    "mocha"                : "^9.1.3",
    "nyc"                  : "^15.1.0",
    "vue"                  : "^2.6.11",
    "webpack"              : "^5.65.0",
    "webpack-cli"          : "^4.9.1"
  }
